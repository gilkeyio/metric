def sumlist(nums list of integer) returns integer
    let total integer = 0
    let i integer = 0
    while i < len(nums)
        set total = total + nums[i]
        set i = i + 1
    return total

def contains(nums list of integer, target integer) returns boolean
    let i integer = 0
    while i < len(nums)
        if nums[i] â‰¡ target
            return true
        set i = i + 1
    return false

